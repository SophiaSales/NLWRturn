// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Feedback{//cria√ßao da bd 
  //colunas do bd
  id String @id @default(uuid())//primare key 

  type String
  comment String
  screenshot String?

  @@map("feedbacks")//nome da tabela 
}